.tooltip {
    position:relative;
}
.tooltip .txt {
    display:none;
    position:absolute;
    z-index:1000;
    color:#fff;
    overflow: hidden;
    height: 180px;
    width: 480px;
    padding: 5px;
    border: 2px white solid;
    border-radius: 4px;
    background:rgba(0,0,0,0.75);
}
/*
.tooltip:hover { background-color: #494949 !important;}*/

.tooltip .txt:before {
    width:0px;
    padding:0px;
    position:absolute;
    content:'';
}

.tooltip .txt img{
    height: 100%;
    float: left;
    max-width: 120px;
}
.tooltip .txt div{
    padding: 5px;
    margin-left: 10px;
    float: left;
    max-width: 340px;
    background: none;
}
.tooltip .txt span{
    font-size: 12px;
}
.tooltip .txt a{
    font-size: 30px;
}

.tooltip.top .txt {
    bottom:20px;
    left:-10px;
}
/* Right */
.tooltip.right .txt {
    top:-200px;
    left:30%;
}
/* Bottom */
.tooltip.bottom .txt {
    top:20px;
    left:-10px;
}
/* Left */
.tooltip.left .txt {
    top:-10px;
    right:20px;
}


