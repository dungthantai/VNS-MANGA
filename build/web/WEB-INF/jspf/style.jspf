    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
}

.topbar_left{
    float:left;
    /*width:50%;*/
}

.topbar_left>*{
    float:left;
    margin-left:5px;
}

.topbar_right{
    float:right;
    /*width:50%;*/
}

.topbar_right div{
